{
	"env": {
		"node": false,
		"browser": true,
		"jasmine": true
	},
	"globals": {
		"angular": true,
		"google": true,
		"inject": true
	},
	"rules": {
		"ident": [0, 4],
		"semi": [ 2, "always"],
		"quotes": [0, "single"],
		"linebreak-style": [0, "unix"],
		"no-unused-vars": [2, { "args": "none" }],

		"no-mixed-spaces-and-tabs": 0
	},
	"extends": "eslint:recommended"
}